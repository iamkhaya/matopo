<%= simple_form_for(@provider) do |f| %>
  <%= f.input :name %>
  <%= f.input :email_1 %>
  <%= f.input :email_2 %>
  <%= f.input :phone_1 %>
  <%= f.input :phone_2 %>
  <%= f.input :tax_number %>
  <%= f.input :street_number %>
  <%= f.input :street_name_1 %>
  <%= f.input :city %>
  <%= f.input :province %>
  <%= f.input :country %>
  <%= f.input :website %>
  <%= f.button :submit, class: "btn-primary" %>
<% end %>

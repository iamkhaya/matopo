<%= simple_form_for( [provider, offering]) do |f| %>

  <%= f.input :activity, collection: Activity.all %>
  <%= f.input :description %>
  <%= f.input :place %>
  <%= f.input :pricingperperson, label: 'Price per Person' %>
  <%= f.input :inclusions,label: 'Whats included'%>
  <%= f.input :exclusions, label: 'Whats not included' %>

  <%= f.button :submit, class: "btn-primary" %>
<% end %>

<h1>New Activity</h1>
<%= form_for(@activity) do |f| %>
<p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
</p>
<p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
</p>

<p>
    <%= f.label :category %><br>
    <%= f.text_field :category%>
</p>
<%= f.submit %>
<% end %>

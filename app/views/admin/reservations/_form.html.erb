<%= simple_form_for([:admin, @reservation]) do |f| %>
  <%= f.association :trip %>

  <%= f.input :user_id %>
  <%= f.input :offering_id, collection: Offering.all%>
  <%= f.input :booking_date %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :status %>

  <%= f.button :submit, class: "btn-primary" %>
<% end %>
